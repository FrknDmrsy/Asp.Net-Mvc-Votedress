            margin: 9px auto;
            border: 1px solid black;
        }

        .colors input {
            position: static;
            opacity: 0;
            cursor: pointer;
        }

        .colors:hover input ~ .checkmarkColor {
            background-color: #ccc;
        }

        /* When the radio button is checked, add a blue background */
        .colors input:checked ~ .checkmarkColor {
            border: 2px solid black;
        }

        /* Create the indicator (the dot/circle - hidden when not checked) */
        .checkmarkColor:after {
            content: "";
            position: absolute;
            display: none;
        }

