    .product-box iframe {
        width: 100%;
        height: 180px;
    }

    .count-input {
        position: relative;
        width: 100%;
        max-width: 165px;
        margin: 10px 0;
    }

        .count-input input {
            width: 100%;
            height: 36.92307692px;
            border: 1px solid #000;
            border-radius: 2px;
            background: none;
            text-align: center;
        }

            .count-input input:focus {
