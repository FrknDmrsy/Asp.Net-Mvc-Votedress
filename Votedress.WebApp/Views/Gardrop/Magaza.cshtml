        .productSize {
            display: block;
            position: relative;
            padding-left: 35px;
            margin-bottom: 5px;
            cursor: pointer;
            font-size: 22px;
            -webkit-user-select: none;
            -moz-user-select: none;
            -ms-user-select: none;
            user-select: none;
        }

            /* Hide the browser's default radio button */
            .productSize input {
                position: absolute;
                opacity: 0;
                cursor: pointer;
            }

        /* Create a custom radio button */
        .checkmarkSize {
            position: absolute;
            top: 0;
            left: 0;
            height: 25px;
            width: 25px;
            background-color: #eee;
            border-radius: 50% !important;
        }

        /* On mouse-over, add a grey background color */
        .productSize:hover input ~ .checkmarkSize {
            background-color: #ccc;
        }

        /* When the radio button is checked, add a blue background */
        .productSize input:checked ~ .checkmarkSize {
            background-color: #17c4bb;
        }

        /* Create the indicator (the dot/circle - hidden when not checked) */
        .checkmarkSize:after {
            content: "";
            position: absolute;
            display: none;
        }

        /* Show the indicator (dot/circle) when checked */
        .productSize input:checked ~ .checkmarkSize:after {
            display: block;
        }

        /* Style the indicator (dot/circle) */
        .productSize .checkmarkSize:after {
            top: 9px;
            left: 9px;
            width: 8px;
            height: 8px;
            border-radius: 50% !important;
            background: white;
        }



